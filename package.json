{
  "devDependencies": {
    "cypress": "^15.7.0",
    "cypress-mochawesome-reporter": "^4.0.2",
    "mochawesome": "^7.1.3",
    "mochawesome-merge": "^4.2.0",
    "mochawesome-report-generator": "^6.2.0"
  },
  "scripts": {
    "cypress:run:report": "npx cypress run --reporter cypress-mochawesome-reporter --reporter-options reportDir=cypress/reports,overwrite=false,html=false,json=true,embeddedScreenshots=true,inlineAssets=true",
    "report:merge": "npx mochawesome-merge cypress/reports/*.json > cypress/reports/report.json",
    "report:generate": "npx marge cypress/reports/report.json -f report -o cypress/reports/html",
    "report:all": "npm run cypress:run:report && npm run report:merge && npm run report:generate"
  }
}
